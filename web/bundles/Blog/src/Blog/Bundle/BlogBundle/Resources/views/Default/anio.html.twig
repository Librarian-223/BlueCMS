{% extends "BlogBlogBundle:Default:index.html.twig" %}

{% block contenido %}
    
        {% for entrada in entradas %}
        {% if entrada.fecha |date("Y") == anio%}
         <div id="entry">
          <div id="titulo"><h1>{{entrada.titulo}}</h1></div>
        <div id="texto">{{entrada.contenido}}</div>
        <br /><br /><p><b>Fecha: </b>{{ entrada.fecha |date("d/M/Y")}}</p>
        </div>
        {% endif %}
        {% endfor %}
        
    

</div>
{% endblock %}